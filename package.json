{
  "name": "dev-sjhomer-com",
  "version": "1.0.0",
  "private": true,
  "description": "SJ Homer - Code Wizard",
  "engines": {
    "node": "16.14.2",
    "npm": "8.5.0"
  },
  "keywords": [
    "gatsby"
  ],
  "workspaces": [
    "./src/*",
    "./web/*"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "start": "concurrently -n gat,sb \"npm run dev\" \"npm run sb\"",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "sb": "NODE_ENV=production start-storybook -p 6006",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
